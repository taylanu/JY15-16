\pard\plain\ltrpar\ql\s30\itap0{\s30\f0\fs24\lang1033{\*\listtag0}\abinodiroverride\ltrch public class ArrayPriorityQueue implements PriorityQueue \line \{ \line   private List items; \line   public ArrayPriorityQueue() \{ items = new ArrayList(); \} \line   public boolean add(Object x) \{ items.add(x); \} \line   public boolean isEmpty() \{ return items.size() == 0; \} \line   public Object remove() \{ Object min = peekMin(); items.remove(min); return min;\} \line   public Object peek() \line   \{ \line     int minIndex = 0; \line     for (int i = 1; i < items.size(); i++) \line     \{ \line       if (((Comparable) items.get(i)).compareTo(items.get(minIndex)) < 0) \line       \{ \line          minIndex = i; \line       \} \line     \} \line     return items.get(minIndex); \line   \} \line \}}{\s30\f0\fs24\lang1033{\*\listtag0}\par}
